<h1>Listing transactions</h1>

<table class="table">
  <thead>
    <tr>
      <th>From</th>
      <th>To</th>
      <th>Amount</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |trans| %>
      <tr>


        <td><%= trans.values[2] %></td>
        <td><%= trans.values[3] %></td>
        <td><%= trans.values[1] %></td>
        <td><%= trans.values[4] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>